source ~/dane/bin/activate

which python

echo ""
echo ""
echo ""

srun -N -n 224 python input.py --no-progress_bar --mode=numba --no_caching --clear_cache --output  surface_c5g7

echo ""
echo ""
echo "*****JOB COMPLETE******"
echo ""
echo ""